<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Bank Utility library for dealing with BBAN and IBAN account numbers plus Finnish and International reference numbers.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="bic-codes.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="bic-codes.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
